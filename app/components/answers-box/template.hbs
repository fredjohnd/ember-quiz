<div class="row">
  {{#each randomizedAnswers as |answer index|}}
  <div class="col-md-6">
    {{answers-box/item
     answer=answer
     index=index
     correctAnswer=correctAnswer
     chosenAnswer=chosenAnswer
     disabled=disabled
     questionIndex=questionIndex
     pickAnswer=attrs.pickAnswer}}
  </div>
  {{/each}}
</div>
